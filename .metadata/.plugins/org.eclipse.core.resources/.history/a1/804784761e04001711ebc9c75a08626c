package lab5;

public class MyThread extends Thread {

	private char _c; 
    /**
     * Default Constructor 
     */
    public MyThread(char c) {
    	_c = c; 
    }

    /**
     * Run override  
     */
    public void start() {
    	for (int i = 0; i < 1000; i++) {
			System.out.println(_c);
		}
    }
    
}
