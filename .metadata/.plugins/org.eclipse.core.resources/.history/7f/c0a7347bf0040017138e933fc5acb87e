package lab5;

public class MyThread extends Thread {

	private char c; 
    /**
     * Default Constructor 
     */
    public MyThread(char c) {
    	this.c = c; 
    }

    /**
     * Run override  
     */
    public void run() {
    	for (int i = 0; i < 1000; i++) {
			System.out.print(this.c);
			try {
				Thread.sleep(1);
			} catch (Exception e) {
				// TODO: handle exception
			}
		}
    }
    
}
